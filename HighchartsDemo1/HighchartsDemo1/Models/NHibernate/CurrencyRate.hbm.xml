<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="HighchartsDemo1" namespace="HighchartsDemo1.Models">
  <class name="CurrencyRate" table="Sales.CurrencyRate" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="ID" column="CurrencyRateID" type="int">
      <generator class="native" />
    </id>
    <property name="Date" column="CurrencyRateDate"/>
    <property name="FromCurrencyCode" column="FromCurrencyCode"/>
    <property name="ToCurrencyCode" column="ToCurrencyCode"/>
    <property name="AverageRate" column="AverageRate"/>
  </class>
</hibernate-mapping>
