<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="HighchartsDemo1" namespace="HighchartsDemo1.Models">
  <class name="Sales" table="Sales_table" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="Id" column="Id" type="int">
      <generator class="native" />
    </id>
    <property name="Country" />
    <property name="Year" />
    <property name="Sales_count" />
  </class> 
</hibernate-mapping>
